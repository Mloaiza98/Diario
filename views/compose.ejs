<%- include("partials/header"); -%>


<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="mb-3 form-group">
    
        <label for="composeTitleId" class="form-label">Title</label>
        <input type="text" class="form-control" name="composeTitle" id="composeTileId">
    </div>
    <div class="mb-3 form-group">
        <label for="composeTextId" class="form-label">Content</label>
        <textarea class="form-control" name="composeText" id="composeTextId" rows="3"></textarea>
    </div>
    <button class="btn btn-primary" type="submit">Publish</button>
        
</form>




<%- include("partials/footer"); -%>